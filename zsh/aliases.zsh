alias \
  -- -="cd -" \
  cl="clear" \
  code="cd $HOME/code" \
  dot="cd $DOTDIR" \
  ejectall="osascript -e 'tell application \"Finder\" to eject (every disk whose ejectable is true)'" \
  ga="git add" \
  gap="git add --patch" \
  gd="git diff" \
  gcm="git checkout master" \
  gco="git checkout" \
  gf='git fetch' \
  gl="git pull" \
  gp="git push" \
  gr="git reset" \
  gs="git status" \
  g="lazygit" \
  stash="git stash" \
  pop="git stash pop" \
  ls="gls --almost-all --color --group-directories-first --human-readable -o" \
  lg="lazygit" \
  n="$EDITOR $HOME/Documents/notes.md" \
  nci="npm clean-install" \
  ni="npm install" \
  nise="npm install --save-exact" \
  nrb="npm run build" \
  nrd="npm run dev" \
  ns="npm start" \
  nt="npm test" \
  nu="npm uninstall" \
  sm="smerge ./" \
  so="source ~/.zshrc; clear" \
  up="zsh $DOTDIR/scripts/update.zsh" \
  v="$EDITOR" \
  x="exit"
